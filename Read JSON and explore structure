import pandas as pd
import json
from pathlib import Path

# Path to your saved JSON file
json_path = Path("/tmp/energy_prices.json")

# Load JSON
with open(json_path, "r", encoding="utf-8") as f:
    data = json.load(f)

# Inspect the structure to see whatâ€™s inside
print(type(data))
print(data.keys() if isinstance(data, dict) else None)
